extends layout

block content
	link(rel="stylesheet", href="/css/main.css")
	section#main_section
				button#btn_insert 올리기
				#img_list
					each vo,index in GALLERY 
						img(src="/images/"+vo.g_filename)
				#modal.modal_overlay
					.modal_window
						form#fileUp(action='/upload', method='POST' enctype="multipart/form-data")
								.filebox
									label(for="userfile") IMG
									input#userfile(type='file', name="userfile")
								input.image_send(name='g_title' placeholder="제목")
								input.image_send(name='g_writer' placeholder="작성자")
								button(type='submit') 완료

